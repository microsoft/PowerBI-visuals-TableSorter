{
  "name": "@essex/tablesorter-powerbi",
  "version": "0.10.8",
  "description": "",
  "private": true,
  "main": "src/index.js",
  "typings": "src/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "tsc": "(tsc > build.out) || (! cat build.out | grep -v \"node_modules.*\" | grep -v \"^\\s.*\") || ! printf \"\\n\\033[0;31mCheck build.out for full error text\\n\"",
    "pbibundle": "gulp",
    "lint": "tslint 'src/**/*[!.d].{ts,tsx}'",
    "karma:tdd": "karma start --tdd",
    "karma:once": "karma start",
    "docs": "typedoc --out docs --ignoreCompilerErrors ./ --externalPattern **/node_modules/**/*.* --exclude **/src/**/*.spec.ts > /dev/null || exit 1",
    "test": "npm-run-all clean tsc lint karma:once docs pbibundle"
  },
  "author": "Microsoft",
  "license": "MIT",
  "dependencies": {
    "@essex/tablesorter": "^0.10.8"
  }
}
